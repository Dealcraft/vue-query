{
  "name": "vue-queryify",
  "private": false,
  "version": "1.0.0-beta.1",
  "author": "David <community@dealcraft.de> (https://dealcraft.de)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dealcraft/vue-queryify.git"
  },
  "bugs": {
    "url": "https://github.com/Dealcraft/vue-queryify/issues"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/vue-queryify.umd.js",
  "module": "./dist/vue-queryify.es.js",
  "types": "./dist/types/vue-querify.es.d.ts",
  "exports": {
    ".": {
      "import": "./dist/vue-queryify.es.js",
      "require": "./dist/vue-queryify.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint --ext .ts,.js,.vue --ignore-path .gitignore --fix src",
    "format": "prettier .  --write",
    "prepare": "npm run build"
  },
  "dependencies": {
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/cli-plugin-eslint": "^5.0.8",
    "@vue/eslint-config-typescript": "^12.0.0",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-vue": "^9.17.0",
    "prettier": "^3.0.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-dts": "^3.6.0",
    "vite-plugin-eslint": "^1.8.1",
    "vue-eslint-parser": "^9.3.1",
    "vue-tsc": "^1.8.5"
  },
  "keywords": ["Vue", "Vue3", "Vue plugin", "Vuejs", "Typescript", "Query", "Web"]
}
